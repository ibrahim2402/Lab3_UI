@model IEnumerable<WebUI.Model.Transaction>

@{
    ViewBag.Title = "Index";
}

<h2>List of Transactions</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TransactionDate)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TransactionDate)
        </td>
       
    </tr>
}

</table>
